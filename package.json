{
  "name": "marden-seo-website",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "Professional SEO services website with integrated Stripe + Mercury payment system",
  "homepage": "https://mardenseo.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kr8thor/stellar-seo-website-design"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --outDir dist/client && node prerender-simple.js && node fix-crawlability.mjs",
    "build:crawlable": "npm run build",
    "build:ssr": "vite build --outDir dist/client && vite build --ssr src/entry-server.jsx --outDir dist/server && node prerender.js",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
    "prerender": "node prerender.js",
    "build:dev": "vite build --mode development",
    "build:ssg": "node build-ssg-crawlable.mjs",
    "build:integrated": "node build-react-ssg-integration.mjs",
    "build:comprehensive": "node build-comprehensive-static-ssg.mjs",
    "build:true-ssg": "node build-true-ssg.mjs",
    "build:final-fix": "node build-final-fix.mjs",
    "build:final-fix-proper": "node build-final-fix-proper.mjs",
    "build:simplified-ssg": "node build-simplified-ssg.mjs",
    "test:simplified": "npm run build:simplified-ssg && npx serve dist -p 8101",
    "build:working": "node build-working-ssg.mjs && node generate-sitemap.mjs",
    "build:complete": "npm run build && node build-complete-ssg.mjs",
    "test:ssg": "npm run build:ssg && npx serve dist -p 8096",
    "test:integrated": "npm run build:integrated && npx serve dist -p 8095",
    "test:comprehensive": "npm run build:comprehensive && npx serve dist -p 8099",
    "test:true-ssg": "npm run build:true-ssg && npx serve dist -p 8100",
    "test:final-fix": "npm run build:final-fix && npx serve dist -p 8097",
    "test:crawlable": "npm run build && npx serve dist -p 8090",
    "test:local-canonicals": "npm run build:comprehensive && node fix-local-canonicals.mjs local && npx serve dist -p 57368",
    "restore:production-canonicals": "node fix-local-canonicals.mjs production",
    "test:working": "npm run build:working && npx serve dist -p 8098",
    "test:complete": "npm run build:complete && npx serve dist -p 8088",
    "build:spa-fix": "node build-react-spa-fix.mjs",
    "test:spa-fix": "npm run build:spa-fix && npx serve dist -p 8097",
    "fix:seo": "node fix-seo-complete.mjs",
    "fix:crawlability": "node fix-crawlability.mjs",
    "fix:canonicalization": "node fix-seo-no-canonicalization.mjs",
    "fix:htaccess": "node fix-htaccess-canonicalization.mjs",
    "verify:seo": "node verify-seo-health.mjs",
    "verify:crawlability": "node test-crawlability.mjs",
    "create:missing": "node create-missing-pages.mjs",
    "generate:missing": "node generate-missing-pages.mjs",
    "build:fix": "npm run build:comprehensive && npm run fix:seo",
    "test:seo-health": "node test-seo-health.mjs",
    "test:fix": "npm run build:fix && npx serve dist -p 8090",
    "complete:seo": "npm run create:missing && npm run fix:seo && npm run verify:seo",
    "sitemap": "node generate-sitemap.mjs",
    "sitemap:validate": "node generate-sitemap.mjs && echo 'Sitemap validation complete'",
    "robots": "node generate-robots.mjs",
    "serve:static": "npx serve dist -p 8097",
    "lint": "eslint .",
    "preview": "vite preview",
    "lovable:dev": "vite --port 5173 --host ::",
    "lovable:build": "vite build --mode development",
    "lovable:preview": "vite preview --port 5173 --host ::"
  },
  "lovable": {
    "projectId": "c99afeda-cbc8-4b8a-acd8-ca805aa26fa8",
    "framework": "react",
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "directEdit": true,
    "syncEnabled": true
  },
